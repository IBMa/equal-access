{
    "name": "report-react",
    "version": "3.0.0",
    "private": true,
    "dependencies": {
      "@babel/core": "7.16.5",
      "@carbon/themes": "^10.48.0",
      "@svgr/webpack": "6.1.2",
      "@testing-library/jest-dom": "^5.16.1",
      "@testing-library/react": "^12.1.2",
      "@testing-library/user-event": "^13.5.0",
      "@typescript-eslint/eslint-plugin": "^5.7.0",
      "@typescript-eslint/parser": "^5.7.0",
      "babel-eslint": "10.0.3",
      "babel-jest": "^27.4.5",
      "babel-loader": "8.2.3",
      "babel-plugin-named-asset-import": "^0.3.8",
      "babel-preset-react-app": "^10.0.1",
      "camelcase": "^6.2.1",
      "carbon-components": "^10.50.0",
      "carbon-components-react": "^7.50.0",
      "carbon-icons": "^7.0.7",
      "case-sensitive-paths-webpack-plugin": "2.4.0",
      "css-loader": "6.5.1",
      "dotenv": "10.0.0",
      "dotenv-expand": "5.1.0",
      "eslint": "^8.4.1",
      "eslint-config-react-app": "^7.0.0",
      "eslint-loader": "3.0.2",
      "eslint-plugin-flowtype": "8.0.3",
      "eslint-plugin-import": "2.25.3",
      "eslint-plugin-jsx-a11y": "6.5.1",
      "eslint-plugin-react": "7.27.1",
      "eslint-plugin-react-hooks": "^4.3.0",
      "express": "^4.17.1",
      "file-loader": "6.2.0",
      "fs-extra": "^10.0.0",
      "html-webpack-plugin": "5.5.0",
      "identity-obj-proxy": "3.0.0",
      "jest": "27.4.5",
      "jest-environment-jsdom-fourteen": "1.0.1",
      "jest-resolve": "27.4.5",
      "jest-watch-typeahead": "1.0.0",
      "markdown-to-jsx": "^7.1.5",
      "mini-css-extract-plugin": "2.4.5",
      "morgan": "^1.10.0",
      "optimize-css-assets-webpack-plugin": "6.0.1",
      "pnp-webpack-plugin": "1.7.0",
      "postcss-flexbugs-fixes": "5.0.2",
      "postcss-loader": "6.2.1",
      "postcss-normalize": "10.0.1",
      "postcss-preset-env": "7.0.2",
      "postcss-safe-parser": "6.0.0",
      "react": "^17.0.2",
      "react-app-polyfill": "^3.0.0",
      "react-dev-utils": "12.0.0",
      "react-dom": "^17.0.2",
      "react-scripts": "5.0.0",
      "resolve": "1.20.0",
      "resolve-url-loader": "^4.0.0",
      "sass-loader": "12.4.0",
      "semver": "7.3.5",
      "style-loader": "3.3.1",
      "terser-webpack-plugin": "5.3.0",
      "ts-pnp": "1.2.0",
      "url-loader": "4.1.1",
      "webpack": "5.65.0",
      "webpack-dev-server": "^4.6.0",
      "webpack-manifest-plugin": "4.0.2",
      "workbox-webpack-plugin": "6.4.2"
    },
    "scripts": {
      "start": "npm run build:help && cross-env NODE_ENV=development SASS_PATH=node_modules:src:node_modules/carbon-components/scss/globals/scss/vendor node scripts/start.js",
      "build:help": "gulp --gulpfile ./gulp/gulpFile.js",
      "build": "npm i && npm run build:react && node build.js",
      "build:react": "npm run build:help && cross-env CI=false NODE_ENV=production SASS_PATH=node_modules:src:node_modules/carbon-components/scss/globals/scss/vendor node scripts/build.js",
      "test": "node scripts/test.js",
      "prod": "PORT=8080 HOST=http://localhost: node ./server.js",
      "analyze": "analyzeChunks=true node scripts/build.js"
    },
    "eslintConfig": {
      "extends": "react-app",
      "rules": {
        "react/jsx-pascal-case": "off",
        "jsx-a11y/anchor-is-valid": "off",
        "no-script-url": "off"
      }
    },
    "browserslist": {
      "production": [
        ">0.2%",
        "not dead",
        "not op_mini all"
      ],
      "development": [
        "last 1 chrome version",
        "last 1 firefox version",
        "last 1 safari version"
      ]
    },
    "jest": {
      "roots": [
        "<rootDir>/src"
      ],
      "collectCoverageFrom": [
        "src/**/*.{js,jsx,ts,tsx}",
        "!src/**/*.d.ts"
      ],
      "setupFiles": [
        "react-app-polyfill/jsdom"
      ],
      "setupFilesAfterEnv": [
        "<rootDir>/src/setupTests.js"
      ],
      "testMatch": [
        "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
        "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
      ],
      "testEnvironment": "jest-environment-jsdom-fourteen",
      "transform": {
        "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
        "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
        "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
      },
      "transformIgnorePatterns": [
        "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
        "^.+\\.module\\.(css|sass|scss)$"
      ],
      "modulePaths": [],
      "moduleNameMapper": {
        "^react-native$": "react-native-web",
        "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
      },
      "moduleFileExtensions": [
        "web.js",
        "js",
        "web.ts",
        "ts",
        "web.tsx",
        "tsx",
        "json",
        "web.jsx",
        "jsx",
        "node"
      ],
      "watchPlugins": [
        "jest-watch-typeahead/filename",
        "jest-watch-typeahead/testname"
      ]
    },
    "babel": {
      "presets": [
        "react-app"
      ]
    },
    "devDependencies": {
      "@types/carbon-components-react": "^7.46.1",
      "@types/carbon__icons-react": "^10.31.2",
      "@types/markdown-to-jsx": "^7.0.0",
      "@types/react": "^17.0.37",
      "@types/react-dom": "^17.0.11",
      "cross-env": "^7.0.3",
      "gulp": "^4.0.2",
      "gulp-ext-replace": "^0.3.0",
      "gulp-replace": "^1.1.3",
      "node-sass": "^7.0.0",
      "source-map-loader": "^3.0.0",
      "ts-loader": "^9.2.6",
      "typescript": "^4.5.4",
      "webpack-bundle-analyzer": "^4.5.0",
      "resize-observer-polyfill": "^1.5.1",
      "use-resize-observer": "^8.0.0"
    }
  }
  