{
  "name": "@ibma/aat-boilerplate",
  "version": "3.0.0",
  "description": "Example usage of IBM AAT with Jest",
  "main": "src/app.js",
  "scripts": {
    "test": "node_modules/jest/bin/jest.js --detectOpenHandles test/*.test.js"
  },
  "engines": {
    "node": ">=10"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./setupAfterEnv.js"
    ],
    "resolver": "jest-pnp-resolver",
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": ["ace-node\\.js"]
},
  "devDependencies": {
    "@babel/cli": "7.19.3",
    "@babel/core": "7.20.2",
    "@babel/plugin-external-helpers": "7.18.6",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-optional-chaining": "7.18.9",
    "@babel/plugin-transform-runtime": "7.19.6",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@babel/runtime": "7.20.1",
    "@babel/standalone": "7.20.4",
    "@carbon/react": "1.0.1",
    "@carbon/themes": "^11.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "11.2.7",
    "@testing-library/user-event": "^14.4.3",
    "accessibility-checker": "^3.1.45",
    "jest": "27.5.1",
    "jest-pnp-resolver": "1.2.2",
    "react": "17.0.1",
    "react-app-polyfill": "1.0.4",
    "react-dom": "17.0.1"
  }
}
